{
  "manifest_version": 2,
  "name": "Random Image Popup",
  "version": "1.0",
  "description": "A Chrome extension that pops up an image at random intervals.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": {
    "extension_pages": "popup.html"
  }
}
